{
  "name": "netlify-webpack-plugin",
  "version": "0.0.1",
  "description": "Webpack Plugin for Netlify",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "webpack",
    "netlify",
    "redirects",
    "headers",
    "_redirects",
    "_headers",
    "plugin"
  ],
  "repository": "https://github.com/RateGravity/netlify-webpack-plugin.git",
  "author": "RateGravity <dev@rategravity.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config rollup.config.js && tsc --build tsconfig.build.json",
    "test": "jest --config jest.config.js",
    "type-check": "tsc --noEmit",
    "webpack": "webpack --config example/webpack.config.js",
    "format": "prettier --write src/**/*.ts",
    "lint": "tslint --project ."
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.1.5",
    "@types/memory-fs": "^0.3.2",
    "babel-jest": "^25.2.6",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.2.6",
    "memory-fs": "^0.5.0",
    "prettier": "^2.0.2",
    "raw-loader": "^0.5.1",
    "rollup": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0",
    "tapable": "^1.1.3",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.1.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {}
}